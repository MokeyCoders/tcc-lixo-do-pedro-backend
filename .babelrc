{
  "comments": false,
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "14"
        }
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "alias": {
          "~": "./src"
        }
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
