{
  "name": "herbario-api",
  "version": "0.1.0",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint:typescript": "tsc --noEmit",
    "lint:eslint": "eslint --cache --fix --ext '.js,.ts,.tsx' .",
    "lint": "run-p lint:typescript lint:eslint",
    "start": "nodemon --ext 'js,ts' --exec babel-node --extensions '.js,.ts,.tsx' src/index.ts",
    "build:clean": "rimraf dist",
    "build:app": "babel src --out-dir dist/app -x '.js,.ts,.tsx' --copy-files --no-copy-ignored",
    "build:database": "babel database --out-dir dist/database -x '.js,.ts,.tsx' --copy-files --no-copy-ignored",
    "build:debug": "npm-run-all -s build:clean \"build:app --source-maps\"",
    "dist": "npm-run-all -s build:clean -p build:app build:database",
    "prepare": "npx husky install",
    "migrate:make": "knex migrate:make --knexfile ./database/knexfile.ts -x ts",
    "migrate:latest": "babel-node ./node_modules/.bin/knex migrate:latest --knexfile ./database/knexfile.ts",
    "migrate:rollback": "babel-node ./node_modules/.bin/knex --knexfile ./database/knexfile.ts migrate:rollback"
  },
  "dependencies": {
    "@casl/ability": "5.4.3",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "date-fns": "2.28.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "joi": "17.5.0",
    "jsonwebtoken": "8.5.1",
    "knex": "0.95.14",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "morgan": "1.10.0",
    "multer": "1.4.4",
    "mysql2": "2.3.3",
    "nodemailer": "6.7.2",
    "objection": "2.2.17",
    "sharp": "0.29.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.5",
    "@babel/node": "7.15.4",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-typescript": "7.15.0",
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "8.5.6",
    "@types/lodash.omit": "4.5.6",
    "@types/lodash.pick": "4.4.6",
    "@types/morgan": "1.9.3",
    "@types/multer": "1.4.7",
    "@types/node": "14.14.31",
    "@types/nodemailer": "6.4.4",
    "@types/sharp": "0.29.5",
    "@types/uuid": "8.3.3",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "14.0.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-import-helpers": "1.1.0",
    "eslint-plugin-jest": "24.4.2",
    "husky": "7.0.4",
    "nodemon": "2.0.12",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "typescript": "4.4.3"
  },
  "packageManager": "yarn@3.1.1"
}
